{
  "author": "TSavo",
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "@wopr-network/plugin-types": "^0.7.0",
    "winston": "^3.11.0"
  },
  "description": "Slack integration for WOPR - Socket Mode and HTTP webhook support",
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/node": "^25.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "plugin",
    "slack",
    "bot",
    "chat"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-slack",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "dev": "tsx src/index.ts",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "test": "vitest run",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
