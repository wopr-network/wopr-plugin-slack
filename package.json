{
  "author": "TSavo",
  "dependencies": {
    "@slack/bolt": "^4.6.0",
    "winston": "^3.11.0"
  },
  "description": "Slack integration for WOPR - Socket Mode and HTTP webhook support",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "plugin",
    "slack",
    "bot",
    "chat"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "wopr-plugin-slack",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "dev": "tsx src/index.ts",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "test": "vitest run",
    "watch": "tsc --watch"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0"
}
